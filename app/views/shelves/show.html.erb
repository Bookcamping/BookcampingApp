<% title shelf.name %>
<% if Shelf::VISUALIZATIONS.include? shelf.visualization %>
  <%= render "shelves/as_#{shelf.visualization}", shelf: shelf %>
<% else %>
  <%= render 'shelves/as_list', shelf: shelf %>
<% end %>

<%= content_for :left do %>
    <%= render 'left' %>
<% end %>

