<% title shelf.name %>
<% if shelf.visualization == 'visit' %>
  <%= render 'shelves/as_visit', shelf: shelf %>
<% elsif shelf.visualization == 'publisher' %>
  <%= render 'shelves/as_publisher', shelf: shelf %>
<% else %>
  <%= render 'shelves/as_list', shelf: shelf %>
<% end %>

<%= content_for :left do %>
    <%= render 'left' %>
<% end %>

<%= content_for :right do %>
  <%= render 'right' %>
<% end %>

