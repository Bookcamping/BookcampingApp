<%= simple_form_for user do |form| %>
  <%= form.input :name %>
<%= form.input :email %>
<%= form.input :password, required: true %>
<%= form.input :password_confirmation, required: true %>
<%= form.input :twitter %>
<%= form.input :description, as: :text %>
<%= form.input :uid_twitter, as: :hidden %>
<%= form.input :uid_facebook, as: :hidden %>
<%= form.input :uid_google, as: :hidden  %>
<%= form.submit 'Darse de alta' %>
  <% end %>
